<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Push Notification Using Web-Push</title>
</head>
<body>
    <h1>Push Notification Using Web-Push</h1>
    <h1 id="user"></h1>
    <script src="./client.js"></script>
    <script>
        let data ={
            "username": "olverarce01@gmail.com",
            "password": "12345"
        }
        const options = {
        method: "POST",
        headers: {
            "Content-Type": "application/json"
        },
        body: JSON.stringify(data)
        };

        // PeticiÃ³n HTTP
        fetch("https://backend-sistemaalertas-production.up.railway.app/login", options)
        .then(response => response.text())
        .then(data => {
            console.log(data);
            document.getElementById("user").textContent=data;
        });
    </script>
</body>
</html>